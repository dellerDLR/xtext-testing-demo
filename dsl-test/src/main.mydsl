    package main {
    	
	    import common.*
	     
	    entity Blog {
	        title: String 
	        many posts: Post
	    }
	     
	       
	    entity Post extends common.HasAuthor { 
	        title: String
	        content: String
	        many comments: Comment
	    }
	     
	    entity Comment extends HasAuthor { 
	        content: String
	    }

  }
    
  